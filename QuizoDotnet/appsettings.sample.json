{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "AspNetCore": "Warning",
      "EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:<APPLICATION_PORT>",
  "AccessTokenKey": "<ACCESS_TOKEN_KEY>",
  "ConnectionStrings": {
    "PostgresConnectionString": "Server=localhost;Port=5432;Database=<DB_NAME>;UserId=<DB_USER>;Password=<DB_PASSWORD>;MaxPoolSize=10;"
  }
}
